{
  "name": "birdpaper-ui",
  "version": "1.0.0-beat.6",
  "description": "提供基于Vue3的UI组件库",
  "scripts": {
    "gulp": "gulp -f build/gulpStyle.js",
    "vite": "vite build --config build/lib.config.js",
    "build": "npm run vite && npm run gulp"
  },
  "dependencies": {
    "gulp-concat": "^2.6.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-sourcemaps": "^3.0.0",
    "vue": "^3.2.11"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.0.1",
    "gulp": "^4.0.2",
    "gulp-less": "^5.0.0",
    "less": "^3.13.1",
    "less-loader": "^5.0.0",
    "vite": "^2.7.7"
  },
  "license": "MIT",
  "main": "dist/birdpaper-ui.umd.js",
  "module": "dist/birdpaper-ui.es.js",
  "style": "dist/index.css",
  "exports": {
    ".": {
      "import": "./dist/birdpaper-ui.es.js",
      "require": "./dist/birdpaper-ui.umd.js"
    },
    "./*": "./*"
  },
  "sideEffects": [
    "dist/*"
  ],
  "files": [
    "dist"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
