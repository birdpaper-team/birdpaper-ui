{
  "private": true,
  "name": "birdpaper-ui-workspaces",
  "packageManager": "pnpm@8.10.4",
  "workspaces": [
    "packages/*",
    "docs",
    "scripts"
  ],
  "scripts": {
    "docs": "pnpm run -C docs dev",
    "build": "pnpm run -C packages build",
    "scripts:dev": "pnpm run -C scripts dev",
    "scripts:build": "pnpm run -C scripts build",
    "build:docs": "pnpm run -C docs build",
    "build:docs-root": "pnpm run -C docs build:root",
    "preview:docs": "pnpm run -C docs preview",
    "yalc": "pnpm run build && yalc publish --push",
    "commit": "git-cz"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@birdpaper-ui/components": "workspace:*",
    "@vueuse/core": "^9.1.0",
    "async-validator": "^4.2.5",
    "birdpaper-icon": "^0.1.0",
    "dayjs": "^1.11.10",
    "vue-virtual-scroller": "2.0.0-beta.8"
  },
  "devDependencies": {
    "@types/node": "^18.19.25",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@pnpm/types": "^8.4.0",
    "@types/fs-extra": "^9.0.13",
    "@types/gulp": "^4.0.9",
    "@types/jsdom": "^16.2.14",
    "@vue/tsconfig": "^0.1.3",
    "tsx": "^4.7.1",
    "typescript": "^4.7.4",
    "vue": "^3.2.37",
    "cz-customizable": "^7.0.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
